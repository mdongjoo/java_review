package user;

import java.util.ArrayList;

public class UserService {
//	UserService 클래스 
//	회원정보를 저장할 list를 private 로 선언한다
	//필드 
	private ArrayList list; 
//생성자 
	public UserService(ArrayList list) {
	super();
	this.list = list;
}
//getter 
	public ArrayList getList() {
		return this.list;
	}
	
	
//	메소드 
	//아이디 중복 검사 메소드 매개변수 list에 있는 아이디를 
	//회원가입 메소드
	//로그인 메소드 
	
	
}
