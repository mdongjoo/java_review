package fileTest;

import java.io.File;
import java.io.IOException;

public class FileTest1 {
	public static void main(String[] args) {
		//자바에서 기본 현재 경로는 프로젝트 폴더이다 
		
		File f = new File("test.txt");
		try {
			f.createNewFile();
			System.out.println("파일 생성되었습니다");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
	}
}
