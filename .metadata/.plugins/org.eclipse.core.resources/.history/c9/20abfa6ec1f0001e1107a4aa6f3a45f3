package map;

import java.util.HashMap;

public class HashMapTest2 {
	public static void main(String[] args) {
		HashMap<String, Integer> mart = new HashMap<>();
		//추가
		mart.put("새우깡",1500);
		mart.put("초코칩쿠키",2500);
		mart.put("포테토칩",2500);
		mart.put("칸쵸",2000);
		
		System.out.println(mart);
	}
}
