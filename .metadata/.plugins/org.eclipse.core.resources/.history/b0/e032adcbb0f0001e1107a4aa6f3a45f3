package set;

import java.util.HashSet;

public class HashSetTest2 {
	public static void main(String[] args) {
		//hash set 순서보장 x 중복 허용 x
		HashSet<String> set = new HashSet<>();
		
		//add 추가 메소드 
		set.add("java");
		set.add("dbms");
		set.add("jdbc");
		set.add("web");
		set.add("jsp");
		set.add("spring boot");
		System.out.println(set);
		
		//remove(Object o) : 지정된 요소를 세트에서 제거 
		set.remove("java");
		System.out.println(set);
		
	}
}
